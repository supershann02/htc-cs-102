<Window x:Class="Week_3_code_along.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week_3_code_along"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock FontSize="30" Height="100">Dog Name: </TextBlock>
            <TextBox Name="DogName"></TextBox>
            <Button Click="Bark_Button_Click" Height="40" Width="100">Bark</Button>
            <Button Click="SayName_Button_Click" Height="40" Width="100">Say Name</Button>
        </StackPanel>
    </Grid>
</Window>
