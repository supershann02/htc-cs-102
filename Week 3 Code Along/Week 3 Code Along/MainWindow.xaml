<Window x:Class="Week_3_Code_Along.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week_3_Code_Along"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock FontSize="50">Dog Name: </TextBlock>
            <TextBox Name="DogName" Height="50"></TextBox>
            <Button Height="50" FontSize="30" Click="Bark_Button_Click">Bark</Button>
            <Button Height="50" FontSize="30" Click="SayName_Button_Click">Say Name</Button>
        </StackPanel>
    </Grid>
</Window>
